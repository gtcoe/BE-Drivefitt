{
  "name": "BE-Drivefitt",
  "version": "1.0.0",
  "description": "Backend API for DriveFitt application",
  "main": "build/src/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node build/src/index.js",
    "dev": "ts-node-dev --respawn --transpile-only app/src/index.ts",
    "dev:watch": "nodemon --exec ts-node app/src/index.ts",
    "clean": "rimraf build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint app/src/**/*.ts",
    "lint:fix": "eslint app/src/**/*.ts --fix"
  },
  "keywords": [
    "nodejs",
    "express",
    "typescript",
    "mysql",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "@types/moment": "^2.11.29",
    "@types/sharp": "^0.31.1",
    "@types/underscore": "^1.13.0",
    "async_hooks": "^1.0.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "json2xls": "^0.1.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "node-cache": "^5.1.2",
    "sharp": "^0.34.2",
    "underscore": "^1.13.7",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/json2xls": "0.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
